PRE INCREMENT
public class Main
{
	public static void main(String[] args) {
	    int a=10;
	    int b=++a;
		System.out.println(a);
		System.out.println(b);
	}
}
POST INCEMENT

public class Main
{
	public static void main(String[] args) {
	    int a=10;
	    int b=a++;
		System.out.println(a);
		System.out.println(b);
	}
}
PRE DECREMENT
public class Main
{
	public static void main(String[] args) {
	    int a=10;
	    int b=--a;
		System.out.println(a);
		System.out.println(b);
	}
}
POST DECREMENT
public class Main
{
	public static void main(String[] args) {
	    int a=10;
	    int b=a--;
		System.out.println(a);
		System.out.println(b);
	}
}
